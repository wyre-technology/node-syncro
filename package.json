{
  "name": "@asachs01/node-syncro",
  "version": "0.0.0-development",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "description": "Comprehensive, fully-typed Node.js/TypeScript library for the Syncro MSP API",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "syncro",
    "syncro-msp",
    "msp",
    "psa",
    "rmm",
    "api",
    "typescript",
    "ticketing",
    "invoicing"
  ],
  "author": "Aaron Sachs",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asachs01/node-syncro.git"
  },
  "bugs": {
    "url": "https://github.com/asachs01/node-syncro/issues"
  },
  "homepage": "https://github.com/asachs01/node-syncro#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitest/coverage-v8": "^1.3.0",
    "eslint": "^8.56.0",
    "msw": "^2.1.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.3.0"
  },
  "dependencies": {}
}
